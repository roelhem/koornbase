        "cross-env": "^5.1.5",
        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "fullcalendar": "^3.9.0",
        "tabler-ui": "github:tabler/tabler",
        "vue-full-calendar": "^2.5.4",
        "vue-moment": "^3.2.0",
        "vue2-google-maps": "^0.9.7",
        "vue2-typeahead": "^1.0.30",
        "vuedraggable": "^2.16.0",
